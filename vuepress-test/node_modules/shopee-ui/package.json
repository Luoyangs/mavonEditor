{
  "name": "shopee-ui",
  "version": "2.8.6",
  "description": "A set of components written with Vue2.0",
  "main": "lib/shopee-ui.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "src",
    "packages",
    "types",
    "component-list.json"
  ],
  "scripts": {
    "new": "node bin/new-component.js",
    "dev": "webpack serve --config build/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --color --config build/webpack.prod.config.js",
    "build-comp": "cross-env NODE_ENV=production-comp webpack --progress --color --config build/webpack.comp.config.js",
    "build-theme": "cross-env NODE_ENV=production webpack --progress --color --config build/webpack.theme.config.js",
    "build-remote": "cross-env NON_BABEL_TRANSFORM=true NODE_ENV=production webpack --progress --color --config build/webpack.remote.config.js",
    "dev-doc": "webpack serve --config build/webpack.doc.config.js",
    "build-doc": "node build/build-doc.js",
    "release": "node bin/release.js",
    "test": "cross-env NODE_ENV=ut karma start test/karma.config.js",
    "test:watch": "cross-env NODE_ENV=ut karma start test/karma.config.js --no-single-run",
    "report-coverage": "codecov --token=907c5690-83a3-4685-ad8e-ca0eb160189f",
    "eslint:all": "eslint --ext .js,.vue packages src examples",
    "eslint:single": "eslint --ext .js,.vue",
    "sass-lint": "sass-lint -v --max-warnings 0",
    "lint": "npm run sass-lint && npm run eslint:all",
    "i18n": "node bin/fetch-transify.js",
    "change-log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "svgo": "svgo -f static/svg -o packages/icon/svg --config .svgo.yml",
    "svg-to-js": "node bin/svg-to-js.js",
    "update-icon": "node bin/update-icon-example.js",
    "dev-snapshots": "webpack serve --config build/webpack.screenshot.config.js",
    "test-jest-snapshots": "jest -c jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://gitlab@git.garena.com:2222/shopee-sz-fe/vue-ui.git"
  },
  "keywords": [
    "component",
    "vue"
  ],
  "author": "Shopee SC FE",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "autoprefixer": "^7.1.4",
    "axios": "^0.18.0",
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "chai": "^4.1.0",
    "chalk": "^3.0.0",
    "codecov": "^3.0.0",
    "commander": "^5.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.8.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "^2.1.0",
    "dashify": "^0.2.2",
    "es6-promise": "^4.1.1",
    "eslint": "^4.3.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-vue": "^7.3.0",
    "file-loader": "^4.0.0",
    "function-bind": "^1.1.0",
    "github-markdown-css": "^2.8.0",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "4",
    "inquirer": "^6.2.0",
    "istanbul": "^0.4.4",
    "jest": "23.6",
    "jest-image-snapshot": "3.1.0",
    "jest-puppeteer": "^3.9.0",
    "json-loader": "^0.5.7",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-html-reporter": "^0.2.7",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "next",
    "lerna": "^2.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "postcss-loader": "^2.0.6",
    "puppeteer": "^1.5.0",
    "request": "^2.85.0",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.6",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.13.0",
    "svg-inline-loader": "^0.8.0",
    "svgo": "^1.0.5",
    "sync-request": "^6.1.0",
    "tinycolor2": "^1.4.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^0.5.7",
    "vue": "2.6.10",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.1.0",
    "vue-i18n": "^8.3.0",
    "vue-loader": "^15.9.5",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^2.7.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "2.6.10",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1",
    "webpack-sources": "^2.1.1",
    "write": "^1.0.3"
  },
  "dependencies": {
    "@shopee-ui/icon": "^2.0.7",
    "fecha": "^2.3.1",
    "popper.js": "^1.14.4"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
